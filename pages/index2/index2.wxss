/* pages/index2/index2.wxss */
page{
  width: 100%;
  height: 100vh;
  background-color: #fff;
}
.wrapper{
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
  width: 100%;
  height: 100%;
}
.title, .botton{
  flex: 1;
  width: 100%;
  padding: 60rpx 0;
  text-align: center;
}
.password{
  flex: 8;
  padding: 100px 0;
  font-size: 50px;
  font-weight: bold;
}
.botton text{
  display: inline-block;
  margin: 5rpx 0;
}
.no{
  color: #b9dd08;
}